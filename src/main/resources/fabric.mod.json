{
  "schemaVersion": 1,
  "id": "luckybindings",
  "version": "${version}",
  "name": "LuckyBindings",
  "description": "Plugin for very custom key-binds for LuckyLuuk_ and his friends",
  "authors": ["LuckyLuuk_"],
  "contact": {
    "homepage": "https://modrinth.com/mod/luckybindings",
    "sources": "https://github.com/LuckyLuuk12/LuckyBindings",
    "issues": "https://github.com/LuckyLuuk12/LuckyBindings/issues?q=sort%3Aupdated-desc+is%3Aissue+is%3Aopen"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.luckyluuk.luckybindings.client.LuckyBindingsClient"
    ],
    "main": [
      "me.luckyluuk.luckybindings.LuckyBindings"
    ],
    "modmenu": [
      "me.luckyluuk.luckybindings.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "luckybindings.mixins.json"
  ],
  "depends": {
    "java": ">=21",
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-api": "*",
    "yet_another_config_lib_v3": ">=3.6.2+1.21.4-fabric"
  }
}
