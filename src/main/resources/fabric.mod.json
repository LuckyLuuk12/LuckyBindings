{
  "schemaVersion": 1,
  "id": "luckybindings",
  "version": "${version}",
  "name": "LuckyBindings",
  "description": "Plugin for very custom key-binds for LuckyLuuk_ and his friends",
  "authors": ["LuckyLuuk_"],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.luckyluuk.luckybindings.client.LuckyBindingsClient"
    ],
    "main": [
      "me.luckyluuk.luckybindings.LuckyBindings"
    ],
    "modmenu": [
      "me.luckyluuk.luckybindings.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "luckybindings.mixins.json"
  ],
  "depends": {
    "java": ">=21",
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fabric-api": "*"
  }
}
